# API do Google Gemini
GEMINI_API_KEY=sua_chave_api_gemini_aqui

# GitHub - Autenticação e Repositório
GITHUB_TOKEN=seu_token_github_aqui
GITHUB_OWNER=nome_do_proprietario
GITHUB_REPO=nome_do_repositorio

# GitHub Projects v2 - IDs necessários
# Para obter esses IDs, use a API GraphQL do GitHub ou inspecione o Network tab no navegador
GITHUB_PROJECT_ID=PVT_kwH...
GITHUB_STATUS_FIELD_ID=PVTSSF_lAH...
GITHUB_AREA_FIELD_ID=PVTSSF_lAH...

# Labels do GitHub (opcionais, valores padrão: frontend, backend, fullstack)
FRONTEND_LABEL=frontend
BACKEND_LABEL=backend